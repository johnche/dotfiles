alias gitroot='cd $(git rev-parse --show-toplevel)'
alias gl='git log -n 10'
alias gs='git show'
alias gd='git diff'
alias gdc='git diff --cached'
alias gst='git status'
alias gp='git pull --rebase'
alias gb='git branch'
alias gnp='git --no-pager'

alias tree='tree -I "target|webpack-cache|node_modules|gemini-report"'
alias grep='grep --exclude-dir={target,webpack-cache,node_modules,gemini-report,coverage,dist,.svelte-kit}'
alias telnet='gtelnet'
alias ls='ls --color=always'
alias ll='ls -ahl'
